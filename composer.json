    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "simplesamlphp/composer-module-installer": false
        }
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/mmenu": [
                "bower-asset/jquery.mmenu"
            ],
            "docroot/libraries/fontawesome": [
                "npm-asset/fortawesome--fontawesome"
            ],
            "docroot/libraries/ckeditor5-anchor-drupal": [
                "npm-asset/northernco--ckeditor5-anchor-drupal"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-module-custom",
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-profile-custom"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-theme-custom",
                "type:drupal-custom-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "sites/default/default.services.yml": false,
                "sites/default/default.settings.php": false
            }
        },
        "patches-file": "composer.patches.json",
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "symlinks": {
            "vendor/kenwheeler/slick": "docroot/libraries/slick",
            "vendor/joeldbirch/superfish/dist": "docroot/libraries/superfish/dist",
            "vendor/joeldbirch/superfish/package.json": "docroot/libraries/superfish/package.json",
            "vendor/phayes/geophp": "docroot/modules/contrib/geofield/lib/phayes-geophp/v1.2",
            "vendor/enyo/dropzone": "docroot/libraries/dropzone",
            "vendor/fengyuanchen/cropper": "docroot/libraries/cropper",
            "vendor/npm-asset/a11y-accordion-tabs": "docroot/libraries/a11y-accordion-tabs"
        }
    }
}
