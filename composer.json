{
  "name": "umn/d8-composer",
  "description": "Project template for Drupal 8 sites built with the Lightning distribution.",
  "type": "project",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "drupal/drupal-extension": "^3.2.0",
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "drupal/coder": "8.*",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8",
    "behat/behat": "^3.0",
    "se/selenium-server-standalone": "^2.53",
    "composer/composer": "^1.3",
    "drush/drush": "8.1.11",
    "drupal/console": "^1.0"
  },
  "require": {
    "composer/installers": "^1.0",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "cweagans/composer-patches": "^1.6.0",
    "acquia/acquia-sdk-php": "^0.10.8",
    "acquia/lightning": "~2.1.0",
    "drupal/paragraphs": "^1.0",
    "drupal/simplesamlphp_auth": "^3.0@dev",
    "drupal/rules": "^3.0@alpha",
    "drupal/google_analytics": "^2.1",
    "drupal/devel": "^1.0@RC",
    "drupal/radix_layouts": "^4.0",
    "drupal/back_to_top": "^1.0@beta",
    "drupal/restui": "^1.13",
    "drupal/admin_toolbar": "^1.18",
    "drupal/fontyourface": "^3.0",
    "drupal/address": "^1.0@RC",
    "drupal/linkit": "^5.0",
    "drupal/robotstxt": "^1.0",
    "drupal/simple_gmap": "^1.2",
    "umn/umn_features": "8.1.2",
    "umn/d8_configurations": "dev-dev",
    "umn/lightning_umn": "dev-master",
    "umn/cog_umn": "dev-master",
    "umn/bootstrap_umn": "dev-master",
    "umn/umn_search": "dev-master",
    "umn/crmform": "dev-d8",
    "umn/dors_migrate": "dev-master",
    "symfony/intl": "^3.2",
    "drupal/mathjax": "^2.6",
    "drupal/redirect": "^1.0@alpha",
    "drupal/imagick": "^1.0",
    "drupal/nice_menus": "^1.0@beta",
    "drupal/fast_404": "^1.0@alpha",
    "drupal/slick": "^1.0@RC",
    "drupal/slick_media": "^1.0@RC",
    "drupal/slick_views": "^1.0@RC",
    "drupal/slick_paragraphs": "^1.0@RC",
    "kenwheeler/slick": "^1.6",
    "drupal/slick_video": "^1.0@RC",
    "drupal/slick_extras": "^1.0@beta",
    "dinbror/blazy": "1.8.2",
    "gdsmith/easing": "1.4.1",
    "kporras07/composer-symlinks": "^0.1.0@dev",
    "drupal/field_group": "^1.0@RC",
    "drupal/block_class": "^1.0@alpha",
    "drupal/webform": "^5.0@beta",
    "drupal/lightning_media_facebook": "^1.0",
    "drupal/lightning_media_imgur": "^1.0",
    "drupal/lightning_media_flickr": "^1.0",
    "drupal/lightning_media_d500px": "^1.0",
    "drupal/lightning_media_soundcloud": "^1.0",
    "drupal/lightning_media_tumblr": "^1.0",
    "drupal/lightning_media_spotify": "^1.0",
    "drupal/lightning_media_pinterest": "^1.0",
    "drupal/workbench_access": "^1.0@alpha",
    "drupal/honeypot": "^1.24",
    "drupal/diff": "^1.0@RC",
    "drupal/migrate_plus": "^4.0",
    "drupal/migrate_tools": "^4.0",
    "drupal/iframe": "^1.7",
    "drupal/metatag": "^1.1",
    "drupal/config_inspector": "^1.0@beta",
    "drupal/select_or_other": "^1.0@alpha",
    "drupal/auto_entitylabel": "dev-2.x",
    "drupal/entityqueue": "dev-1.x",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/acquia_purge": "^1.0@beta",
    "drupal/menu_block": "^1.4",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/shield": "^1.1",
    "drupal/memcache": "^2.0@alpha"
  },
  "repositories": {
    "drupal-packagist": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "umn-features": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/d8-features.git"
    },
    "umn-configs": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/d8-configurations.git"
    },
    "lightning_umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalprofiles/lightning_umn.git"
    },
    "cog-umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalthemes/cog-umn.git"
    },
    "bootstrap-umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalthemes/bootstrap-umn.git"
    },
    "umn-search": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/umn-search.git"
    },
    "crmform": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/crmform"
    },
    "dors_migrate": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/dors_migrate.git"
    },
    "blazy": {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "type": "drupal-library",
        "version": "1.8.2",
        "source": {
          "url": "https://github.com/dinbror/blazy.git",
          "type": "git",
          "reference": "1.8.2"
        }
      }
    },
    "jquery.easing": {
      "type": "package",
      "package": {
        "name": "gdsmith/easing",
        "type": "drupal-library",
        "version": "1.4.1",
        "source": {
          "url": "https://github.com/gdsmith/jquery.easing.git",
          "type": "git",
          "reference": "1.4.1"
        }
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  },
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-module-custom"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-profile-custom"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-theme-custom"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "source": "http://cgit.drupalcode.org/drupal/plain/{path}?h={version}",
      "excludes": [ "robots.txt" ]
    },
    "patches-file": "composer.patches.json",
    "enable-patching": true,
    "symlinks": {
      "vendor/kenwheeler/slick": "docroot/libraries/slick"
    }
  }
}
